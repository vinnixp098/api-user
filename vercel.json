{
    "version": 2,
    "builds": [
      {
        "src": "api/user.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/product.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/user.js"
      },
      {
        "src": "/dados-produtos/(.*)",
        "dest": "/api/product.js"
      }
    ]
  }
      